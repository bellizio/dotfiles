function lazygit() {
  git add .
  git commit -a -m "$1"
  git push
}

function upgrade-nvm() {
  (
    cd "$NVM_DIR"
    git fetch origin
    git checkout `git describe --abbrev=0 --tags --match "v[0-9]*" origin`
  ) && . "$NVM_DIR/nvm.sh"
}

function keygen() {
  x=$(ps -A | openssl sha1 | head -c 32 | pbcopy); echo $x
}

function iterm2_print_user_vars() {
  iterm2_set_user_var nodeVersion $(node -v)
}
